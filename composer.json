{
    "name": "detailnet/dfw-normalization",
    "description": "DETAIL Framework library for array/object (de-)normalization",
    "type": "library",
    "keywords": [
        "dfw",
        "detailnet",
        "normalization",
        "serialization",
        "hydration"
    ],
    "homepage": "https://github.com/detailnet/dfw-normalization/",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Wolf",
            "email": "ivan.wolf@detailnet.ch"
        }
    ],
    "require": {
        "php": "^7.0",
        "jms/serializer": "^1.5"
    },
    "require-dev": {
        "ramsey/uuid": "^3.5",
        "phpunit/phpunit": "^4.4.5",
        "squizlabs/php_codesniffer" : "^2.2",
        "phpmd/phpmd": "^2.1.3",
        "satooshi/php-coveralls": "^1.0"
    },
    "suggest": {
        "ramsey/uuid": "For usage of the UUID handler of the JMS Serializer"
    },
    "autoload": {
        "psr-0": {
            "Detail\\Normalization\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "DetailTest\\Normalization\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "archive": {
        "exclude": [
            "!vendor",
            "tests",
            "*phpunit.xml",
            ".coveralls.yml",
            ".travis.yml",
            "build.xml",
            "build.properties",
            "composer.phar"
        ]
    }
}
